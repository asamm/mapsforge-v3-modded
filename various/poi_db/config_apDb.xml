    <params>
        <poi-details>
            <tag key="brand" />
            <tag key="cuisine" />
            <tag key="description" />
            <tag key="opening_hours" />
            <tag key="operator" /> <!-- name a company for hotel, restaurant, shop ... -->
            <tag key="shop" />
            <tag key="sport" />
            <tag key="stars" /> <!-- hotel classification -->
            <tag key="wikipedia" /> <!-- Is human readable title of the article, eg: wikipedia=en:St Paul's Cathedral -->

            <tag key="denomination" />
        </poi-details>

        <poi-email>
            <tag key="contact:email" />
            <tag key="email" />
        </poi-email>
        <poi-phone>
            <tag key="contact:phone" />
            <tag key="phone" />
        </poi-phone>
        <poi-url >
            <tag key="website" /> <!-- OSM suggest to use web site but URL key is also possible-->
            <tag key="url" />
            <tag key="contact:website" />
        </poi-url>
    </params>
</configuration>
